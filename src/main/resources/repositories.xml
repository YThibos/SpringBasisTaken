<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns='http://www.springframework.org/schema/beans'
	xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
	xsi:schemaLocation='http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd'>

	<bean id="csvPersoonRepository" class="be.vdab.repositories.CSVPersoonRepository">
		<constructor-arg value="${csvFileLocation}" />
	</bean>
	<bean id="multilinePersoonRepository" class="be.vdab.repositories.MultilinePersoonRepository">
		<constructor-arg value="${multilineFileLocation}" />
	</bean>


</beans>